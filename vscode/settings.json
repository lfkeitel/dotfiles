{
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.tabSize": 2
    },
    "[markdown]": {
        "editor.quickSuggestions": false
    },
    "[json]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "remote.SSH.showLoginTerminal": true,
    "remote.SSH.useLocalServer": false,
    "C_Cpp.default.cStandard": "c11",
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "markdown",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml"
    ],
    "auto-close-tag.fullMode": true,
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "javascript",
        "javascriptreact"
    ],
    "breadcrumbs.enabled": true,
    "d.dcdClientPath": "/home/lfkeitel/.local/share/code-d/bin/DCD/bin/dcd-client",
    "d.dcdServerPath": "/home/lfkeitel/.local/share/code-d/bin/DCD/bin/dcd-server",
    "d.dubPath": "/home/lfkeitel/.local/share/code-d/bin/dub",
    "d.servedPath": "/home/lfkeitel/.local/share/code-d/bin/serve-d",
    "dart.allowAnalytics": false,
    "dart.lineLength": 120,
    "dart.reportAnalyzerErrors": false,
    "dfmt.braceStyle": "otbs",
    "docker.showExplorer": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.fontFamily": "'Fira Code', Inconsolata, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.minimap.renderCharacters": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.tabSize": 4,
    "elm.analyseEnabled": true,
    "elm.includeUserFunctionDocumentation": true,
    "eslint.packageManager": "yarn",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript"
    ],
    "explorer.confirmDelete": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.asm": "asm",
        "*.el": "lisp",
        "*.exp": "tcl",
        "*.tmpl": "gohtml",
        ".zsh_aliases": "shellscript",
        ".zsh_functions": "shellscript",
        "*.json": "json",
        "Dockerfile_*": "dockerfile",
        "dockerfile-*": "dockerfile",
        "*.html": "html",
        "*.dat": "ledger"
    },
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "gitlens.keymap": "none",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "go.autocompleteUnimportedPackages": true,
    "go.docsTool": "gogetdoc",
    "html.autoClosingTags": true,
    "html.format.endWithNewline": true,
    "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
    "java.errors.incompleteClasspath.severity": "ignore",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "latex-workshop.chktex.enabled": true,
    "ledger.binary": "/usr/bin/ledger",
    "npm.enableScriptExplorer": true,
    "npm.packageManager": "yarn",
    "powershell.integratedConsole.showOnStartup": false,
    "python.pythonPath": "python3",
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv",
        "code/venv"
    ],
    "restructuredtext.linter.extraArgs": [
        "--ignore D001"
    ],
    "rust.all_features": true,
    "rust.build_on_save": true,
    "rust.clippy_preference": "on",
    "rust.unstable_features": true, // Conditionally enable range formatting, not enabled by default
    "rust.all_targets": false, // Don't pass `-all` to `cargo check`, it often takes too much time
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.rendererType": "dom",
    "tslint.packageManager": "yarn",
    "vsicons.dontShowNewVersionMessage": true,
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Cobalt2",
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tips.enabled": false,
    "workbench.list.horizontalScrolling": true,
    "workbench.tree.indent": 8,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.preset": "OTBS",
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "go.useLanguageServer": true,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "vetur.experimental.templateInterpolationService": true,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.view.pdf.viewer": "tab",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "extensions.showRecommendationsOnlyOnDemand": true,
    "python.jediEnabled": false,
    "python.formatting.provider": "black",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
