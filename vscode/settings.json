{
    "[html]": {
        "editor.formatOnSave": false
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": false
    },
    "[jsonc]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.tabSize": 2
    },
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "blade",
        "ejs",
        "jinja",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "markdown",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml"
    ],
    "auto-close-tag.fullMode": true,
    "auto-close-tag.SublimeText3Mode": true,
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "javascript",
        "javascriptreact"
    ],
    "breadcrumbs.enabled": true,
    "dfmt.braceStyle": "otbs",
    "editor.bracketPairColorization.enabled": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "'Fira Code', Inconsolata, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "editor.guides.bracketPairs":"active",
    "editor.insertSpaces": true,
    "editor.minimap.renderCharacters": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "explorer.confirmDelete": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        ".zsh_aliases": "shellscript",
        ".zsh_functions": "shellscript",
        "*.asm": "asm",
        "*.dat": "ledger",
        "*.el": "lisp",
        "*.exp": "tcl",
        "*.html": "html",
        "*.json": "json",
        "*.tmpl": "gohtml",
        "Dockerfile_*": "dockerfile",
        "dockerfile-*": "dockerfile"
    },
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "go.useLanguageServer": true,
    "html.autoClosingTags": true,
    "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
    "java.errors.incompleteClasspath.severity": "ignore",
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "ledger.binary": "/usr/bin/ledger",
    "npm.packageManager": "yarn",
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.preset": "OTBS",
    "powershell.integratedConsole.showOnStartup": false,
    "python.languageServer": "Default",
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv",
        "code/venv"
    ],
    "remote.SSH.showLoginTerminal": true,
    "remote.SSH.useLocalServer": false,
    "rust.all_features": true,
    "rust.all_targets": false,
    "rust.build_on_save": true,
    "rust.clippy_preference": "on",
    "rust.unstable_features": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontSize": 16,
    "tslint.packageManager": "yarn",
    "vetur.experimental.templateInterpolationService": true,
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Cobalt2",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#FFD700",
        "editorBracketHighlight.foreground2": "#DA70D6",
        "editorBracketHighlight.foreground3": "#87CEFA",
        "editorBracketHighlight.foreground4": "#90EE90",
        "editorBracketHighlight.foreground5": "#4B0082",
        "editorBracketHighlight.foreground6": "#556B2F",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.horizontalScrolling": true,
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 8,
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "go.toolsManagement.autoUpdate": true,
    "python.defaultInterpreterPath": "/home/lfkeitel/code/venv/py3/bin/python3",
    "remote.SSH.remotePlatform": {
        "logstash-r16-15": "linux",
        "stuff.keitel.dev": "linux",
        "garden.keitel.dev": "linux",
        "stargate": "linux"
    },
    "powershell.promptToUpdatePowerShell": false,
    "editor.formatOnSave": true
}
