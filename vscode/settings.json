{
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "extensions.ignoreRecommendations": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code, Inconsolata, monospace",
    "editor.fontSize": 17,
    "files.trimTrailingWhitespace": true,
    "docker.showExplorer": false,
    "workbench.colorTheme": "Cobalt2",
    "editor.rulers": [120],
    "[markdown]": {
        "editor.rulers": [
            80,
            130
        ]
    },
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
    "files.associations": {
        "*.tmpl": "gohtml",
        ".zsh_aliases": "shellscript",
        ".zsh_functions": "shellscript",
        "*.exp": "tcl",
        "*.js": "javascriptreact",
        "*.el": "lisp"
    },
    "[javascript]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[javascriptreact]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "html.format.endWithNewline": true,
    "html.autoClosingTags": true,
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.fullMode": true,
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "javascript",
        "javascriptreact"
    ],
    "window.restoreWindows": "none",
    "explorer.confirmDragAndDrop": false,
    "python.pythonPath": "python3",
    "explorer.confirmDelete": false,
    "python.disablePromptForFeatures": [
        "pylint"
    ],
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "d.dubPath": "/home/lfkeitel/.vscode/extensions/webfreak.code-d-0.16.2/bin/dub",
    "d.dscannerPath": "/home/lfkeitel/.vscode/extensions/webfreak.code-d-0.16.2/bin/Dscanner/bin/dscanner",
    "d.dfmtPath": "/home/lfkeitel/.vscode/extensions/webfreak.code-d-0.16.2/bin/dfmt/bin/dfmt",
    "d.dcdClientPath": "/home/lfkeitel/.vscode/extensions/webfreak.code-d-0.16.2/bin/DCD/bin/dcd-client",
    "d.dcdServerPath": "/home/lfkeitel/.vscode/extensions/webfreak.code-d-0.16.2/bin/DCD/bin/dcd-server",
    "dfmt.braceStyle": "otbs",
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascriptreact",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml"
    ],
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "[razor]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "gitlens.keymap": "none",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "terminal.integrated.fontSize": 16,
    "dart.allowAnalytics": false,
    "dart.lineLength": 120,
    "dart.reportAnalyzerErrors": false,
    "dart.showLintNames": true,
    "ledger.binary": "/usr/bin/ledger",
    "restructuredtext.linter.extraArgs": [
        "--ignore D001"
    ],
    "powershell.integratedConsole.showOnStartup": false,
    "mocha.env": {
        "NODE_ENV": "test"
    },
    "mocha.options": {
        "ignoreLeaks": true
    },
    "gitlens.historyExplorer.enabled": true,
    "tslint.packageManager": "yarn"
}
